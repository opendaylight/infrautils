<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
	xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.1.0">
    <reference id="notificationService" interface="org.opendaylight.controller.md.sal.binding.api.NotificationService"/>
    <reference id="rpcRegistry" interface="org.opendaylight.controller.sal.binding.api.RpcProviderRegistry"/>
    
	<bean id="mainclass" init-method="init"	class="org.opendaylight.infrautils.samples.asyncmdsal.consumer.SampleMdsalAsyncNotificationConsumerMain">
        <property name="notificationService" ref="notificationService"/>
    </bean>
    <bean id="mainclass2" init-method="init" class="org.opendaylight.infrautils.samples.asyncmdsal.consumer.SampleMdsalAsyncRpcConsumerMain">
        <property name="rpcRegistry" ref="rpcRegistry"/>
    </bean>
</blueprint>
