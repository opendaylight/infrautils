<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
	xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.1.0">
    <reference id="notificationPublishService" interface="org.opendaylight.controller.md.sal.binding.api.NotificationPublishService"/>
    <reference id="rpcRegistry" interface="org.opendaylight.controller.sal.binding.api.RpcProviderRegistry"/>
    
	<bean id="mainclass" init-method="init"	class="org.opendaylight.infrautils.samples.asyncmdsal.provider.SampleMdsalAsyncNotificationProviderMain">
        <property name="notificationPublishService" ref="notificationPublishService"/>           
    </bean>
    <bean id="mainclass2" init-method="init" class="org.opendaylight.infrautils.samples.asyncmdsal.provider.SampleMdsalAsyncRpcProviderMain">
        <property name="rpcRegistry" ref="rpcRegistry"/>           
    </bean>
</blueprint>
